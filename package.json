{
  "name": "parcel-react-bare-minimum",
  "version": "0.0.1",
  "license": "MIT",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/flexdinesh/parcel-react-bare-minimum"
  },
  "scripts": {
    "dev": "parcel public/*.html --cache-dir .parcel-cache",
    "build": "parcel build public/*.html --cache-dir .parcel-cache --reporter @parcel/reporter-bundle-analyzer",
    "clean": "rimraf --no-glob ./dist ./.parcel-cache ./node_modules/.cache "
  },
  "devDependencies": {
    "@emotion/babel-plugin": "11.7.2",
    "@parcel/packager-ts": "^2.2.1",
    "@parcel/reporter-bundle-analyzer": "^2.3.1",
    "@parcel/reporter-bundle-buddy": "^2.3.1",
    "@parcel/transformer-svg-react": "^2.2.1",
    "@parcel/transformer-typescript-types": "^2.2.1",
    "@types/styled-system": "^5.1.14",
    "parcel": "^2.2.1",
    "rimraf": "3.0.2",
    "typescript": "4.5.4",
    "typescript-styled-plugin": "^0.18.2"
  },
  "dependencies": {
    "@emotion/react": "^11.7.1",
    "@parcel/transformer-react-refresh-wrap": "^2.2.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "6"
  }
}
